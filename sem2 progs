        "N=13\n",
        "D=2\n",
        "Low=1\n",
        "High=13\n",
        "while Low<=High:\n",
        "  Mid=(High+Low)//2\n",
        "  sum_digits=sum(int(i)for i in str(Mid))\n",
        "  diff=(Mid-sum_digits)\n",
        "  if diff<=0:\n",
        "    Low=Mid+1\n",
        "  else:\n",
        "    High=Mid-1\n",
        "    result=N-High\n",
        "print(result)"
